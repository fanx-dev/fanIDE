<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">

<fontscolors>
    <!--lexer-->
    <fontcolor default="keyword" name="keyword"/>
    <fontcolor default="separator" name="separator"/>
    <fontcolor default="comment" name="comment"/>
    <fontcolor default="error" name="error"/>
    <fontcolor default="number" name="number"/>
    <fontcolor default="character" name="character"/>
    <fontcolor foreColor="006f00" name="DOC"/>
    <fontcolor foreColor="0066ff" name="comparator"/>
    <fontcolor foreColor="a606fd" name="assignment"/>
    <fontcolor foreColor="ff0000" name="initialization">
		<font style="bold"/>
	</fontcolor>
    <fontcolor foreColor="ff006f" name="DSL">
	<font style="bold"/>
    </fontcolor>
    <fontcolor default="string" name="STR"/>

    <!-- semantic (parser)-->
    <fontcolor name="mod-parameter">
	<font style="bold"/>
    </fontcolor>
    <fontcolor name="mod-method" underline="000000">
    	<font style="bold"/>
    </fontcolor>
    <fontcolor name="mod-global" underline="000000"/>
    <fontcolor name="mod-constructor" foreColor="550000" underline="550000">
	<font style="bold"/>
    </fontcolor>
    <fontcolor name="mod-class" underline="000000">
	<font style="bold"/>
    </fontcolor>
    <fontcolor name="mod-static">
	<font style="italic"/>
    </fontcolor>
    <!-- custom1 is for 'override' -->
    <fontcolor name="mod-custom1" foreColor="550055"/>
	<!-- custom2 is string interpolation vars -->
    <fontcolor name="mod-custom2">
		<font style="bold"/>
    </fontcolor>
    <!-- not used yet-->
    <fontcolor name="mod-deprecated" strikeThrough="404040"/>
    <fontcolor name="mod-unused" waveUnderlined="999999"/>
    <fontcolor name="mod-field" foreColor="005500"/>
 </fontscolors>