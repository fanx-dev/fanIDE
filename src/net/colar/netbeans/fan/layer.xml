<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Loaders">
	<folder name="text">
	    <folder name="x-fan">
		<folder name="Actions">
		    <file name="net-colar-netbeans-fan-project-SetMainFileAction.instance">
			<attr name="position" intvalue="150"/>
		    </file>
		</folder>
	    </folder>
	</folder>
    </folder>

    <folder name="Services">
	<folder name="MIMEResolver">
	    <file name="fan.xml" url="fanResolver.xml">
		<attr name="position" intvalue="1007"/>
	    </file>
	</folder>
    </folder>

    <folder name="Editors">
	<folder name="text">
	    <folder name="x-fan">
		<file name="language.instance">
		    <attr name="instanceCreate" methodvalue="net.colar.netbeans.fan.FanTokenID.language"/>
		    <attr name="instanceOf" stringvalue="org.netbeans.api.lexer.Language"/>
		</file>
		<folder name="popup">
		    <file name="netc-colar-netbeans-fan-project-SetMainFileAction.instance"/>
		</folder>
	    </folder>
	</folder>
	<folder name="FontsColors">
	    <folder name="NetBeans">
		<folder name="Defaults">
		    <file name="coloring.xml" url="textColors.xml"/>
		</folder>
	    </folder>
	</folder>
    </folder>

    <folder name="CslPlugins">
	<folder name="text">
	    <folder name="x-fan">
		<file name="language.instance">
		    <attr name="instanceClass" stringvalue="net.colar.netbeans.fan.FanLanguage"/>
		</file>
		<file name="structure.instance">
		    <attr name="instanceClass" stringvalue="net.colar.netbeans.fan.structure.FanStructureAnalyzer"/>
		</file>
	    </folder>
	</folder>
    </folder>

    <folder name="Templates">
	<folder name="Fan">
	    <!--<attr name="SystemFileSystem.localizingBundle" stringvalue="org.myorg.additionalfiletemplates.Bundle"/>-->
	    <file name="EmptyFile.fan" url="templates/EmptyFile.html">
		<attr name="template" boolvalue="true"/>
		<attr name="SystemFileSystem.icon" urlvalue="nbresloc:/net/colar/netbeans/fan/fan.png"/>
		<attr name="templateWizardURL" urlvalue="nbresloc:/net/colar/netbeans/fan/templates/EmptyFile.html"/>
                <!--Use this line only if your template makes use of the FreeMarker template language:-->
		<attr name="javax.script.ScriptEngine" stringvalue="freemarker"/>
	    </file>
	    <file name="Class.fan" url="templates/Class.html">
		<attr name="template" boolvalue="true"/>
		<attr name="SystemFileSystem.icon" urlvalue="nbresloc:/net/colar/netbeans/fan/fan.png"/>
		<attr name="templateWizardURL" urlvalue="nbresloc:/net/colar/netbeans/fan/templates/ClassDesc.html"/>
                <!--Use this line only if your template makes use of the FreeMarker template language:-->
		<attr name="javax.script.ScriptEngine" stringvalue="freemarker"/>
	    </file>
	    <file name="MainClass.fan" url="templates/MainClass.html">
		<attr name="template" boolvalue="true"/>
		<attr name="SystemFileSystem.icon" urlvalue="nbresloc:/net/colar/netbeans/fan/fan.png"/>
		<attr name="templateWizardURL" urlvalue="nbresloc:/net/colar/netbeans/fan/templates/MainClassDesc.html"/>
                <!--Use this line only if your template makes use of the FreeMarker template language:-->
		<attr name="javax.script.ScriptEngine" stringvalue="freemarker"/>
	    </file>
	    <file name="fan">
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/net/colar/netbeans/fan/fan.png"/>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="net.colar.netbeans.fan.Bundle"/>
                <attr name="instantiatingIterator" newvalue="net.colar.netbeans.fan.wizard.FanWizardIterator"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/net/colar/netbeans/fan/wizard/fan.html"/>
            </file>
        </folder>
	
	<folder name="Project">
	    <folder name="Fan Project">
		<!--<attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.docbook.project.Bundle"/>-->
		<attr name="position" intvalue="937"/>
		<file name="Standard Fan Project">
		    <!--<attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.docbook.project.Bundle"/>-->
		    <attr name="instantiatingIterator" methodvalue="net.colar.netbeans.fan.wizard.FanWizardIterator.instance"/>
		    <attr name="instantiatingWizardURL" urlvalue="nbresloc:/net/colar/netbeans/fan/project/NewFanProjectDescription.html"/>
		    <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/net/colar/netbeans/fan/fan.png"/>
		    <attr name="template" boolvalue="true"/>
		</file>
		<file name="Fan POD">
		    <!--<attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.docbook.project.Bundle"/>-->
		    <attr name="instantiatingIterator" methodvalue="net.colar.netbeans.fan.project.wizard.PodWizzard.instance"/>
		    <attr name="instantiatingWizardURL" urlvalue="nbresloc:/net/colar/netbeans/fan/project/NewFanPodDescription.html"/>
		    <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/net/colar/netbeans/fan/fan.png"/>
		    <attr name="template" boolvalue="true"/>
		</file>
		<!-- Allow Fan files to show up in Java projects as well. -->
               <!--<attr name="templateCategory" stringvalue="fan,java-classes"/>-->
	    </folder>
	</folder>
    </folder>
</filesystem>
